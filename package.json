    "gridfs-stream": "^1.1.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^9.0.0",
    "mongodb": "^4.7.0",
    "mongoose": "^6.4.3",
    "multer": "^1.4.5-lts.1",
    "multer-gridfs-storage": "^5.0.2",
